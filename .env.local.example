# Configuración de Logging para Debugging
# Copia este archivo a .env.local y ajusta según necesites

# Nivel de logging general: 'off', 'error', 'warn', 'info', 'debug'
VITE_LOG_LEVEL=warn

# Control de categorías específicas (true/false)
# API requests
VITE_LOG_API=false
# Sistema de autenticación  
VITE_LOG_AUTH=false
# Sistema de permisos
VITE_LOG_PERMISSIONS=false
# Cache de TanStack Query
VITE_LOG_CACHE=true
# Mutaciones (nuestro sistema de tracking)
VITE_LOG_MUTATIONS=true
# Redux store
VITE_LOG_REDUX=false
# HTTP requests (muy verbose)
VITE_LOG_HTTP=false
# Logging muy detallado
VITE_LOG_VERBOSE=false

# Configuración del DebugPanel
VITE_DEBUG_PANEL=true
VITE_DEBUG_PANEL_AUTO_OPEN=false
VITE_DEBUG_PANEL_INTERVAL=2000
